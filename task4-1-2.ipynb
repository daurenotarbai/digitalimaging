    "import math\n",
    "\n",
    "def gauss(sigma, x, y):\n",
    "    return 1 / (2 * math.pi * sigma ** 2) * math.exp(-(x ** 2 + y ** 2) / (2 * sigma ** 2))\n",
    "\n",
    "sigma, x, y = [int(_) for _ in input().split()]\n",
    "print(gauss(sigma, x, y))"
   ]
